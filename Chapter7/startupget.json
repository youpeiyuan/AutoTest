[
  {
    "description":"这是没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text":"这是一个没有参数的get请求"
    }
  },
  {
    "description":"这是有参数的get请求",
    "request":{
      "uri":"/getwithdemo",
